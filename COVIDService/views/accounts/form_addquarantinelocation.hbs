<form class="form" action="/account/create_quarantinelocation" method="post" style="width:50vw">
  <h1 class="display-5">ADD NEW QUARANTINE LOCATION</h1>
  <div class="form-group">
    <label for="f_Address">Address</label>
    <input type="text" class="form-control" id="f_Address" placeholder="Address" name="f_Address" />
  </div>
  <div class="form-group">
    <label for="f_Capacity">Capacity</label>
    <input type="number" class="form-control" id="f_Capacity" placeholder="Capacity" name="f_Capacity" />
  </div>
  <div class="form-group">
    <label for="f_CurrentCapacity">Current Capacity</label>
    <input type="number" class="form-control" id="f_CurrentCapacity" placeholder="Current Capacity" name="f_CurrentCapacity" />
  </div>
  {{#if err}}
    <div class="alert alert-danger" role="alert">
      {{err}}
    </div>
  {{/if}}
  <button type="submit" class="btn btn-default btn-success">
    Create
  </button>
</form>